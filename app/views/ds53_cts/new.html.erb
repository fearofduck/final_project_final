      </div>

      <!-- Label and input for oct -->
      <div class="form-group">
        <label for="oct" class="control-label">
          Oct
        </label>

        <input type="text" id="oct" name="oct" placeholder="Oct" class="form-control" value="<%= @ds53_ct.oct %>">
      </div>

      <!-- Label and input for nov -->
      <div class="form-group">
        <label for="nov" class="control-label">
          Nov
        </label>

        <input type="text" id="nov" name="nov" placeholder="Nov" class="form-control" value="<%= @ds53_ct.nov %>">
      </div>

      <!-- Label and input for dec -->
